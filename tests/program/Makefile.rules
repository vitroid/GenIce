2D3.gro: $(FMT)/gromacs.py
	$(GENICE) 2D3 -r 4 4 1 --depol=optimal | fgrep -v "Command line:" > $@
2D3-000.analice.gro: 2D3.gro $(FMT)/yaplot.py
	$(ANALICE) $< -f gromacs -o '2D3-%03d.analice.gro'

